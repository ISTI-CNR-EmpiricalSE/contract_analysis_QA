                       this.txt_file = response.data.txt_file_docx_en\r\n\r\n                        }\r\n\r\n                        this.file_uploaded = response.data.title\r\n\r\n                    })\r\n                    .catch(error => {\r\n                        console.log(error);\r\n                    });\r\n            } else if (file.type === \"application/vnd.openxmlformats-officedocument.spreadsheetml.sheet\") {\r\n                axios\r\n                    .post(\"http://localhost:8000/api/xlsx-upload/\", formData)\r\n                    .then(response => {\r\n                        \r\n                        if (this.language === 'it') {\r\n                            this.text = response.data.xlsx_text_it\r\n                            this.txt_file = response.data.txt_file_xlsx_it\r\n                        } else {\r\n                            this.text = response.data.xlsx_text_en\r\n                            this.txt_file = response.data.txt_file_xlsx_en\r\n                        }\r\n                        this.file_uploaded = response.data.title\r\n                    })\r\n                    .catch(error => {\r\n                        console.log(error);\r\n                    });\r\n            } else {\r\n                alert(\"Unsupported file type\");\r\n            }\r\n        },\r\n        updateText() {\r\n            axios.post(\"http://localhost:8000/api/update-text/\", { text_toEdit: this.editText, file_toEdit: this.txt_file, file_source: this.file_uploaded, language: this.language})\r\n                .then(response => {\r\n                    this.text = this.editText;\r\n                    this.txt_file = response.data.txt_file_edited;\r\n                    this.editing = false;                    \r\n                })\r\n                .catch(error => {\r\n                    console.log(error);\r\n                });\r\n        },\r\n\r\n        loadCnf(){\r\n            axios.post(\"http://localhost:8000/api/load-config/\", {file_txt_path: this.txt_file, language: this.language, text: this.text, f_up: this.file_uploaded})\r\n            .then(response => {\r\n                this.dictionary = JSON.parseresponse.data.jsonDict\r\n            })\r\n            .catch(error => {\r\n                    console.log(error);\r\n                });\r\n        }\r\n        \r\n        \r\n\r\n        \r\n    }\r\n};\r\n</script>\r\n"],"mappings":"AA0CA,OAAAA,KAAA;;AAGA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,QAAA;MACAC,OAAA;MACAC,QAAA;MACAC,aAAA;MACAC,QAAA;MACAC,UAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,IAAAC,QAAA,OAAAC,QAAA;MACAD,QAAA,CAAAE,MAAA,cAAAC,KAAA,CAAAC,IAAA,CAAAC,KAAA;MACAL,QAAA,CAAAE,MAAA,kBAAAN,QAAA;MAEA,IAAAQ,IAAA,QAAAD,KAAA,CAAAC,IAAA,CAAAC,KAAA;MAEA,IAAAD,IAAA,CAAAE,IAAA;QACAjB,KAAA,CACAkB,IAAA,0CAAAP,QAAA,EACAQ,IAAA,CAAAC,QAAA;UAEA,SAAAb,QAAA;YACA,KAAAL,IAAA,GAAAkB,QAAA,CAAAnB,IAAA,CAAAoB,WAAA;YACA,KAAAlB,QAAA,GAAAiB,QAAA,CAAAnB,IAAA,CAAAqB,eAAA;UACA;YACA,KAAApB,IAAA,GAAAkB,QAAA,CAAAnB,IAAA,CAAAsB,WAAA;YACA,KAAApB,QAAA,GAAAiB,QAAA,CAAAnB,IAAA,CAAAuB,eAAA;UACA;UAEA,KAAAlB,aAAA,GAAAc,QAAA,CAAAnB,IAAA,CAAAwB,KAAA;QAGA,GACAC,KAAA,CAAAC,KAAA;UACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;QACA;MACA,WAAAZ,IAAA,CAAAE,IAAA,kFAAAF,IAAA,CAAAE,IAAA;QACAjB,KAAA,CACAkB,IAAA,2CAAAP,QAAA,EACAQ,IAAA,CAAAC,QAAA;UACA,SAAAb,QAAA;YACA,KAAAL,IAAA,GAAAkB,QAAA,CAAAnB,IAAA,CAAA6B,YAAA;YACA,KAAA3B,QAAA,GAAAiB,QAAA,CAAAnB,IAAA,CAAA8B,gBAAA;UACA;YACA,KAAA7B,IAAA,GAAAkB,QAAA,CAAAnB,IAAA,CAAA+B,YAAA;YACA,KAAA7B,QAAA,GAAAiB,QAAA,CAAAnB,IAAA,CAAAgC,gBAAA;UAEA;UAEA,KAAA3B,aAAA,GAAAc,QAAA,CAAAnB,IAAA,CAAAwB,KAAA;QAEA,GACAC,KAAA,CAAAC,KAAA;UACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;QACA;MACA,WAAAZ,IAAA,CAAAE,IAAA;QACAjB,KAAA,CACAkB,IAAA,2CAAAP,QAAA,EACAQ,IAAA,CAAAC,QAAA;UAEA,SAAAb,QAAA;YACA,KAAAL,IAAA,GAAAkB,QAAA,CAAAnB,IAAA,CAAAiC,YAAA;YACA,KAAA/B,QAAA,GAAAiB,QAAA,CAAAnB,IAAA,CAAAkC,gBAAA;UACA;YACA,KAAAjC,IAAA,GAAAkB,QAAA,CAAAnB,IAAA,CAAAmC,YAAA;YACA,KAAAjC,QAAA,GAAAiB,QAAA,CAAAnB,IAAA,CAAAoC,gBAAA;UACA;UACA,KAAA/B,aAAA,GAA